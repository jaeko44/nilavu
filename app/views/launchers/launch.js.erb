$(document).ready(function(){
  removeAt('#prelaunch_modal');
  $('#cockpit').append('<%= escape_javascript(render :partial => "launchers/prelaunch_modal", :locals => {:launch_item => @launch_item}) %>');
  $("#prelaunch_modal").modal('show');
});

$(document).ready(function() {
    $("#megamform").validate({
        rules : {
            check_req : {
                required : true
            }
        },
        messages : {
            check_req : "Just check the box"
        },
         tooltip_options: {
            check_req : {placement:'right',html:true}
         }
    });

});
